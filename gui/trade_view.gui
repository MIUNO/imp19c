				}
				
			}
			
			margin_widget = {
				#margin = { 2 2 }
				size = { 100% 100% }
				button = {
					#texture = "gfx/interface/base_parts/white_background.dds"
					spriteType = Corneredtiled
					alpha = 0.95
					spriteborder = { 200 200 }
					gfxtype = framedbuttongfx
					size = { 100% 100% }
					framesize = { 200 200 }
					upframe = 1
					overframe = 2
					downframe = 3
					disableframe = 4
					parentanchor = center
					
					modify_texture = {
						name = "Alpha"
						texture = "gfx/interface/base_parts/circle_60.dds"
						blend_mode = alphamultiply
						#spriteType = corneredtiled
						#spriteborder = { 6 6 }
					}
					
